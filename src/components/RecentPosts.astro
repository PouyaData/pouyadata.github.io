---
import PostList from '~/components/PostList.astro';
import type { CollectionEntry } from 'astro:content';
interface Props {
  posts: CollectionEntry<'blog'>[];
}
const { posts } = Astro.props as Props;
---

<section class="section">
  <h2>Recent Posts</h2>
  <PostList posts={posts} />
</section>
